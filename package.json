{
  "name": "ui-elements",
  "version": "2.0.1",
  "description": "A repo of ui elements",
  "main": "index.js",
  "scripts": {
    "build": "rimraf dist && npm run installs && gulp webpack && gulp copy",
    "build:dev": "rimraf dist && gulp webpack-dev && gulp copy",
    "lint": "eslint .",
    "installs": "gulp installs",
    "copy": "gulp copy",
    "dev": "npm run build:dev && node index.js",
    "pretest": "node ./test/helpers/download-test-browsers.js",
    "test": "npm run build && npm run lint && mocha",
    "start": "npm run build && node index.js"
  },
  "keywords": [
    "localnerve",
    "ui",
    "elements"
  ],
  "author": "Alex Grant (@localnerve)",
  "license": "BSD-3-Clause",
  "dependencies": {
    "chai": "^3.5.0",
    "chromedriver": "^2.26.1",
    "express": "^4.14.1",
    "geckodriver": "^1.6.0",
    "q": "^1.4.1",
    "selenium-assistant": "^5.0.5"
  },
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "eslint": "^3.14.1",
    "eslint-config-airbnb-base": "^11.0.1",
    "eslint-plugin-import": "^2.2.0",
    "gulp": "^3.9.1",
    "mocha": "^3.3.0",
    "rimraf": "^2.5.4",
    "webpack": "^2.2.1"
  },
  "engines": {
    "node": ">= 6.5"
  }
}
