{
  "name": "ui-elements",
  "version": "9.0.0",
  "description": "A repo of ui elements",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/localnerve/ui-elements.git"
  },
  "scripts": {
    "build": "rimraf dist && npm run installs && gulp webpack && gulp copy",
    "build:dev": "rimraf dist && gulp webpack-dev && gulp copy",
    "lint": "eslint .",
    "installs": "gulp installs",
    "copy": "gulp copy",
    "dev": "npm run build:dev && node index.js",
    "test": "npm run build && npm run lint && playwright test",
    "test:debug": "npm run build && npm run lint && playwright test --headed --timeout=60000",
    "start": "npm run build && node index.js"
  },
  "keywords": [
    "localnerve",
    "ui",
    "elements"
  ],
  "author": "Alex Grant <alex@localnerve.com> (https://www.localnerve.com)",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "@eslint/js": "^9.34.0",
    "@playwright/test": "^1.55.0",
    "babel-loader": "^10.0.0",
    "eslint": "^9.34.0",
    "express": "^5.1.0",
    "globals": "^16.3.0",
    "gulp": "^5.0.1",
    "picklr": "latest",
    "rimraf": "^6.0.1",
    "terser-webpack-plugin": "^5.3.14",
    "webpack": "^5.101.3",
    "webpack-cli": "^6.0.1"
  },
  "overrides": {
    "request": "npm:@cypress/request@3.0.9"
  },
  "engines": {
    "node": ">=20"
  }
}
