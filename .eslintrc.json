{
    "extends": "airbnb-base",
    "plugins": [
        "import"
    ],
    "rules": {
        "function-paren-newline": ["error", "consistent"],
        "space-before-function-paren": ["error", "always"],
        "no-plusplus": 0,
        "comma-dangle": 0,
        "arrow-parens": 0,
        "max-classes-per-file": 0,
        "prefer-object-spread": ["warn"]
    }
}
